{
  "name": "babylon-demo",
  "version": "0.0.1",
  "description": "A simple scene using Babylon.js, typescript and webpack",
  "main": "index.ts",
  "scripts": {
    "start": "webpack serve --config build/webpack.dev.js",
    "dev": "npm start",
    "dll": "webpack --progress --config build/webpack.dll.js",
    "build": "webpack --config build/webpack.prod.js",
    "lint": "npx eslint . --ext .ts,.tsx"
